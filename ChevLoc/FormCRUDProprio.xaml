<Window x:Class="ChevLoc.FormCRUDProprio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FormCRUDProprio" Height="300" Width="400">
    <Grid>
        <Label Content="Nom :" HorizontalAlignment="Left" Margin="44,25,0,0" VerticalAlignment="Top"/>
        <Label Content="Prénom :" HorizontalAlignment="Left" Margin="44,80,0,0" VerticalAlignment="Top"/>
        <Label Content="Sexe :" HorizontalAlignment="Left" Margin="44,138,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnValider" Content="Valider" HorizontalAlignment="Left" Margin="44,219,0,0" VerticalAlignment="Top" Width="75" Click="btnValider_Click"/>
        <Button x:Name="btnAnnuler" Content="Annuler" HorizontalAlignment="Left" Margin="269,219,0,0" VerticalAlignment="Top" Width="75" Click="btnAnnuler_Click"/>
        <TextBox x:Name="tbNom" HorizontalAlignment="Left" Height="23" Margin="224,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbPrenom" HorizontalAlignment="Left" Height="23" Margin="224,83,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cbSexe" HorizontalAlignment="Left" Margin="224,142,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
